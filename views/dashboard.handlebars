<h1>Your Dashboard</h1>
<div>
  <a href="/post/new">Create New Post</a>
  {{#each posts}}
    <article>
      <h2><a href="/post/{{id}}">{{title}}</a></h2>
      <p>{{content}}</p>
      <a href="/post/edit/{{id}}">Edit</a>
      <a href="/post/delete/{{id}}">Delete</a>
    </article>
  {{/each}}
</div>
